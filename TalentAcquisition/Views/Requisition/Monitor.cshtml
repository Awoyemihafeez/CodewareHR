
@{
    ViewBag.Title = "Monitor Requisitions";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script>
$(document).ready(function(){
  $("#myInput").on("keyup", function() {
    var value = $(this).val().toLowerCase();
    $("#RequisitionTable tr").filter(function() {
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
  });
});
</script>
<section class="mainbody" style="overflow-x:hidden">
    <div class="container-fluid" >
        <div class="col-md-3">
            <div class="container">
                @Html.Partial("_AdminDashboardSidebar")
            </div>
        </div>

        <div class="col-md-9" style="margin-left: 22em; width: 79.6%; margin-top: 3em;">
            @*<div style="padding-top:2em;"></div>*@
            <p style="font-family: 'Fira Mono';font-size: 22px;">Requisition Dashboard</p>
            <ol class="breadcrumb">
                <li><a href="@Url.Action("Dashboard","Admin")">Home</a></li>
                @*<li><a href="#">Private</a></li>*@
                @*<li><a href="#">Pictures</a></li>*@
                @*<li class="active"><a href="@Url.Action("Requistionmanager","Admin")">Requisition Manager</a></li>*@
                <li class="active">Job Requisition Manager</li>
            </ol>

            <section class="">
              

                <div class="container-fluid" style="margin:0px 12px">
                    <div class="row">
                        @Html.Action("_GetNotifications", "Admin")
                    </div>
                </div>
            </section>
</div>

    </div>
</section>


